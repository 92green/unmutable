{
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "name": "unmutable",
  "version": "1.0.0-alpha.2",
  "description": "An immutable, point-free, functional data collection library for plain old Javascript.",
  "license": "MIT",
  "author": "Damien Clarke",
  "repository": {
    "type": "git",
    "url": "git@github.com:blueflag/unmutable.git"
  },
  "bugs": {
    "url": "https://github.com/blueflag/unmutable/issues"
  },
  "private": false,
  "scripts": {
    "build": "rm -rf dist && yarn make-index && yarn rollup --config",
    "prepublishOnly": "yarn run build",
    "watch": "yarn run build -w && watch-docs",
    "check-coverage": "NODE_ENV=test nyc check-coverage --branches 100.0  --functions 100.0 --lines 100.0",
    "flow": "blueflag-test flow",
    "flow-coverage": "blueflag-test flow-coverage -M",
    "lint": "blueflag-test lint",
    "make-index": "node scripts/make-index.js",
    "test": "yarn jest --maxWorkers=1",
    "test-all": "yarn lint && yarn flow && yarn test && yarn size-limit",
    "view-coverage": "open ./coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@commitlint/cli": "^7.2.1",
    "babel-preset-blueflag": "^1.0.0",
    "blueflag-test": "^0.22.0",
    "husky": "^1.1.2",
    "immutable": "4.0.0-rc.9",
    "immutable3": "npm:immutable@^3.8.1",
    "rollup": "^1.17.0",
    "rollup-plugin-analyzer": "^3.1.2",
    "rollup-plugin-terser": "^5.1.1",
    "size-limit": "1.0.x"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "fast-deep-equal": "^1.0.0",
    "is-plain-object": "^2.0.4",
    "lodash.range": "^3.2.0",
    "rollup-plugin-babel": "^4.3.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
